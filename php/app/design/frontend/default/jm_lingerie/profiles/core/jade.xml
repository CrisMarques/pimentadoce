<?xml version="1.0" encoding="iso-8859-1"?>
<config>
   <sections>
        <wavethemes_jmbasetheme>
            <label>JM Base theme</label>
            <tab>wavethemes</tab>
            <frontend_type>text</frontend_type>
            <sort_order>10</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
	        	<jmbasethemejade translate="label">
				    <label>JM Basetheme: settings for jade profile</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>99999</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
						<masstop1color translate="label comment">
                            <label>Category List Background Color: </label>
                            <comment>To apply Category List Background color, Category List Image must be transparent or empty</comment>
                            <frontend_type>text</frontend_type>
                            <frontend_model>jmbasetheme/adminhtml_system_config_form_colorpicker</frontend_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </masstop1color>
                        
                        <masstop1image>
                            <label>Category List Background image </label>
                            <frontend_type>image</frontend_type>
                            <backend_model>jmbasetheme/backend_image</backend_model>
                            <upload_dir config="system/filesystem/media" scope_info="1">images</upload_dir>
                            <base_url type="media" scope_info="1">images</base_url>
                            <sort_order>2</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </masstop1image>
						
						
						<massbottomcolor translate="label comment">
                            <label>Ads Footer Background Color: </label>
                            <comment>To apply Ads Footer Background color, Ads Footer Image must be transparent or empty</comment>
                            <frontend_type>text</frontend_type>
                            <frontend_model>jmbasetheme/adminhtml_system_config_form_colorpicker</frontend_model>
                            <sort_order>3</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </massbottomcolor>
                        
                        <massbottomimage>
                            <label>Ads Footer Background image </label>
                            <frontend_type>image</frontend_type>
                            <backend_model>jmbasetheme/backend_image</backend_model>
                            <upload_dir config="system/filesystem/media" scope_info="1">images</upload_dir>
                            <base_url type="media" scope_info="1">images</base_url>
                            <sort_order>4</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </massbottomimage>
						
						<botslcolor translate="label comment">
                            <label>Spotlight Background Color: </label>
                            <comment>To apply Spotlight Background color, Spotlight Image must be transparent or empty</comment>
                            <frontend_type>text</frontend_type>
                            <frontend_model>jmbasetheme/adminhtml_system_config_form_colorpicker</frontend_model>
                            <sort_order>5</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </botslcolor>
                        
                        <bostslimage>
                            <label>Spotlight Background image </label>
                            <frontend_type>image</frontend_type>
                            <backend_model>jmbasetheme/backend_image</backend_model>
                            <upload_dir config="system/filesystem/media" scope_info="1">images</upload_dir>
                            <base_url type="media" scope_info="1">images</base_url>
                            <sort_order>6</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </bostslimage>
						
						<buttonpreslideshow>
                            <label>Button Back of Slideshow </label>
